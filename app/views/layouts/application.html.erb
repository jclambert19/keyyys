<!DOCTYPE html>
<html>
  <head>
    <title>Keyyys</title>
   <!--  <link rel="icon" type="image/x-icon" href="keyyys-favincon.png"> -->
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= csrf_meta_tags %>
    <%= favicon_link_tag "keyyys-favincon.png" %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  </head>
  <body>
    <div id="wrapper">
      <%= render 'shared/navbar' %>
      <%= render 'shared/flashes' %>
      <%= yield %>
    </div>


    <div class="bottom">
      <div class="container">
        <div class="row">
            <p>Copyright 2015 Keyyys | Tous droits réservés</p>
        </div>
      </div>
    </div>


    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?libraries=places&key=<%= ENV['GOOGLE_API_KEY'] %>"></script>

    <%= javascript_include_tag "application" %>

    <%= yield :after_js %>
  </body>
</html>
